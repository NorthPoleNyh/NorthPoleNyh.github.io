function get_codemirror_mode(lang){switch(lang){case 'C++':case 'C++O2':case 'C++17':return'text/x-c++src';case 'C':return'text/x-csrc';case 'Python2.7':case 'Python3':return'text/x-python';case 'Java7':case 'Java8':return'text/x-java';case 'Pascal':return'text/x-pascal';case 'text':return'text/plain';default:return'text/plain'}};function require_codemirror_mode(mode,callback){var name='none';switch(mode){case 'text/x-c++src':case 'text/x-csrc':case 'text/x-java':name='clike';break;case 'text/x-python':name='python';break;case 'text/x-pascal':name='pascal';break}
